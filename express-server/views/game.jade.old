extends layout

block content
  #wrapper
    #opponent
    #head
      h1 Welcome to Guess Who?!?
    #guessWindow.list
    #chatWindow.list
    center
      table#board
    div
      form(name='guessForm')
        input#guessInput(type='text', name='guessItem', onkeypress='return runGuess(event)')
        #guessButton Guess!
      form(name='chatForm')
        input#chatInput(type='text', name='chatItem', onkeypress='return runChat(event)')
      #chatButton Send!
  #yourCard
    center
